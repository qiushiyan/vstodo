#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/qiushi/workspace/vscode-extensions/vstodo-api/node_modules/.pnpm/typeorm@0.2.41_pg@8.7.1/node_modules/typeorm/node_modules:/Users/qiushi/workspace/vscode-extensions/vstodo-api/node_modules/.pnpm/typeorm@0.2.41_pg@8.7.1/node_modules:/Users/qiushi/workspace/vscode-extensions/vstodo-api/node_modules/.pnpm/node_modules:/Users/qiushi/workspace/vscode-extensions/vstodo-api/node_modules:/Users/qiushi/workspace/vscode-extensions/node_modules:/Users/qiushi/workspace/node_modules:/Users/qiushi/node_modules:/Users/node_modules:/node_modules:/Users/qiushi/workspace/vscode-extensions/vstodo-api/node_modules/typeorm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/qiushi/workspace/vscode-extensions/vstodo-api/node_modules/.pnpm/typeorm@0.2.41_pg@8.7.1/node_modules/typeorm/node_modules:/Users/qiushi/workspace/vscode-extensions/vstodo-api/node_modules/.pnpm/typeorm@0.2.41_pg@8.7.1/node_modules:/Users/qiushi/workspace/vscode-extensions/vstodo-api/node_modules/.pnpm/node_modules:/Users/qiushi/workspace/vscode-extensions/vstodo-api/node_modules:/Users/qiushi/workspace/vscode-extensions/node_modules:/Users/qiushi/workspace/node_modules:/Users/qiushi/node_modules:/Users/node_modules:/node_modules:/Users/qiushi/workspace/vscode-extensions/vstodo-api/node_modules/typeorm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typeorm/cli.js" "$@"
else
  exec node  "$basedir/../typeorm/cli.js" "$@"
fi
