#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/qiushi/workspace/vscode-extensions/vstodo-api/node_modules/.pnpm/ts-node@9.1.1_typescript@4.5.4/node_modules/ts-node/dist/node_modules:/Users/qiushi/workspace/vscode-extensions/vstodo-api/node_modules/.pnpm/ts-node@9.1.1_typescript@4.5.4/node_modules/ts-node/node_modules:/Users/qiushi/workspace/vscode-extensions/vstodo-api/node_modules/.pnpm/ts-node@9.1.1_typescript@4.5.4/node_modules:/Users/qiushi/workspace/vscode-extensions/vstodo-api/node_modules/.pnpm/node_modules:/Users/qiushi/workspace/vscode-extensions/vstodo-api/node_modules:/Users/qiushi/workspace/vscode-extensions/node_modules:/Users/qiushi/workspace/node_modules:/Users/qiushi/node_modules:/Users/node_modules:/node_modules:/Users/qiushi/workspace/vscode-extensions/vstodo-api/node_modules/.pnpm/node_modules/ts-node/dist/node_modules:/Users/qiushi/workspace/vscode-extensions/vstodo-api/node_modules/.pnpm/node_modules/ts-node/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/qiushi/workspace/vscode-extensions/vstodo-api/node_modules/.pnpm/ts-node@9.1.1_typescript@4.5.4/node_modules/ts-node/dist/node_modules:/Users/qiushi/workspace/vscode-extensions/vstodo-api/node_modules/.pnpm/ts-node@9.1.1_typescript@4.5.4/node_modules/ts-node/node_modules:/Users/qiushi/workspace/vscode-extensions/vstodo-api/node_modules/.pnpm/ts-node@9.1.1_typescript@4.5.4/node_modules:/Users/qiushi/workspace/vscode-extensions/vstodo-api/node_modules/.pnpm/node_modules:/Users/qiushi/workspace/vscode-extensions/vstodo-api/node_modules:/Users/qiushi/workspace/vscode-extensions/node_modules:/Users/qiushi/workspace/node_modules:/Users/qiushi/node_modules:/Users/node_modules:/node_modules:/Users/qiushi/workspace/vscode-extensions/vstodo-api/node_modules/.pnpm/node_modules/ts-node/dist/node_modules:/Users/qiushi/workspace/vscode-extensions/vstodo-api/node_modules/.pnpm/node_modules/ts-node/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-node/dist/bin-script.js" "$@"
else
  exec node  "$basedir/../ts-node/dist/bin-script.js" "$@"
fi
